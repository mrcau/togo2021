 : cf.f2(); })\r\n    return ref3.off();\r\n  }\r\n  //Î≥¥Í≥†ÏÑú ÌÖåÏù¥Î∏î Ïã±ÌÅ¨\r\nasync reportSync(folder,roomId, cf) {\r\n    if (!roomId) { return }\r\n    // const roomId = roomName.substr(0,6)+'REPORT'\r\n    const ref1 = fireInit.database().ref(`${folder}/${roomId}`);\r\n    ref1.on('value', (p) => {\r\n      const data = p.val()||{};\r\n      const Data = Object.values(data);\r\n      data ? cf.f1(Data) : cf.f2();\r\n    });\r\n  }\r\n// ÎπÑÎîîÏò§ Î©îÏãúÏßÄ Ïã±ÌÅ¨\r\nasync videoSync(folder,roomName,spot,cf) {\r\n  const roomUid = roomName.substr(0,roomSubstr);\r\n  if (!roomUid) { return }\r\n  const ref = fireInit.database().ref(`${folder}/${roomUid}/${spot}`);\r\n  ref.on('value', (p) => { \r\n    const data = p.val();\r\n    cf(data);\r\n  });\r\n  return ()=>ref.off();\r\n}\r\n // Item ÏîΩÌÅ¨\r\n itemSync(folder, uid, cf) {\r\n  const ref = fireInit.database().ref(`${folder}/${uid}`);\r\n  ref.on('value', (p) => {\r\n    const data = p.val();\r\n    data ? cf.f1(data) : cf.f2();\r\n  })\r\n}  \r\n\r\n}\r\n\r\n\r\nexport default firesync\r\n","C:\\Users\\User\\Desktop\\Coding\\togo2021\\src\\service\\fireproblem.js",[],"C:\\Users\\User\\Desktop\\Coding\\togo2021\\src\\service\\fire.js",[],"C:\\Users\\User\\Desktop\\Coding\\togo2021\\src\\component\\leftMenu\\LeftMenu.jsx",[],"C:\\Users\\User\\Desktop\\Coding\\togo2021\\src\\component\\loginModal\\LoginModal.jsx",["171","172"],"import './loginModal.css';\r\nimport React, { memo, useRef, useState } from 'react';\r\nimport { Button, Form } from 'react-bootstrap';\r\nimport MailOutlineIcon from '@material-ui/icons/MailOutline';\r\nimport LockOpenIcon from '@material-ui/icons/LockOpen';\r\nimport InsertEmoticonIcon from '@material-ui/icons/InsertEmoticon';\r\nimport Swal from 'sweetalert2';\r\nimport google from './goolge.png';\r\n\r\nfunction LoginModal({ fireApp, setuser,moveModal4,registerTF, setRegisterTF }) {\r\n  const emailRef = useRef();\r\n  const passRef = useRef();\r\n  const emailRegisterRef = use