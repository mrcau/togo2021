.off();\r\n}\r\n// 룸네임 없으면 컬러 업데이트\r\nitemColorUp(folder, uid,dataId, color) {\r\nfireInit.database().ref(`${folder}/${uid}/${dataId}`)\r\n  .update({ color: color })\r\n}\r\n // mytool 업데이트\r\n opentoolUp(folder,uid, dataId, counter) {\r\n  fireInit.database().ref(`${folder}/${uid}/${dataId}`)\r\n    .update({ progress: counter })\r\n}\r\n// mytool 삭제\r\nopentoolDel(folder,uid,dataId) {\r\n  fireInit.database().ref(`${folder}/${uid}/${dataId}`).remove();\r\n}\r\n\r\n\r\n\r\n}\r\nexport default firesync\r\n\r\n","C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\firetodo.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\firelogin.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\fireopentool.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\fireproblem.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\fireidea.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\App.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\service\\fire.js",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\component\\mytool\\Mytoolbox.jsx",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\component\\authTable\\Atable.jsx",[],"C:\\Users\\mrcau\\OneDrive\\문서\\GitHub\\togo2021\\src\\component\\leftMenu\\LeftMenu.jsx",["200","201","202"],"import './leftMenu.css';\r\nimport { Avatar,  IconButton } from '@material-ui/core';\r\nimport { BatteryCharging20, BatteryCharging50, BatteryCharging80, BatteryChargingFull, PhotoCamera } from '@material-ui/icons';\r\nimport React, { memo } from 'react';\r\nimport {  Link, useHistory } from 'react-router-dom';\r\nimport mime from 'mime-types';\r\nimport { Accordion, Card } from 'react-bootstrap';\r\n\r\n\r\nfunction LeftMenu({ fireApp, user, photo, setPhoto,userInfo, logout,moveModal2,setlogoName }) {\r\nconst upLoad = (e) => {\r\n    const file = e.target.files[0];\r\n    const metaData = { contentType: mime.lookup(fi